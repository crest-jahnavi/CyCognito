        <name>Minimize DateField Width</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Function minimizes the width of date field
    try {
        var dateHtml = g_form.getElement("from_date").parentElement;
        dateHtml.parentElement.style.cssText = "width:200px !important";
    } catch (err) {
        alert("error in setting width for date field");
    }

}]]></script>
