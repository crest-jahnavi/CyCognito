        <name>FutureDateNotAllowed</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	//Function checks whether from date is less than or equal to today's date or not.

    if (isLoading || newValue == '') {

        return;

    }

    var today = new Date();

    var start = new Date(g_form.getValue('from_date'));

    //g_form.hideFieldMsg('from_date');

    if (start > today)
    {
        g_form.showFieldMsg('from_date', 'Start date must be in the future.', 'error');
        g_form.clearValue('from_date');

    }



}]]></script>
